<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.czw.eshop.entity.User" table="tb_u" schema="shop">
        <id name="userID" column="u_id"/>
        <property name="userName" column="u_name"/>
        <property name="userPwd" column="u_pwd"/>
        <property name="userAddr" column="u_addr"/>
        <property name="userSex" column="u_sex"/>
        <property name="userEmail" column="u_mail"/>
        <property name="userPhone" column="u_phone"/>
        <property name="userType" column="u_type"/>
    </class>
</hibernate-mapping>